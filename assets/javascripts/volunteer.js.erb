(function() {
    Discourse.Dialect.inlineBetween({
        start: "[volunteer:",
        stop:  "[/volunteer]",
        rawContents: true,
        emitter: function(contents) {
            var matches = contents.match(/([a-zA-Z]+)\]([0-9]*)/);
            var type = matches[1];
            var id = matches[2];
            return ['button', {'class':'btn','id': id,'volunteer-type': type} ].concat('volunteer');
        }
    });
  Discourse.Markdown.whiteListTag('button', 'id');
  Discourse.Markdown.whiteListTag('button', 'volunteer-type');
  Discourse.Markdown.whiteListTag('button', 'class','btn');

}).call(this);