(function() {

  Discourse.Dialect.inlineRegexp({
    start: '[volunteer:',
    matcher: /(.+)]([\s\S]*)[\/volunteer]/,
    emitter: function(contents) {
      var type = contents[1];
      var id = contents[2];
      return ['button', {"id": id} ].concat('volunteer');
    }
  });
  Discourse.Markdown.whiteListTag('button', 'id');

}).call(this);