(function() {
    Discourse.Dialect.inlineBetween({
        start: "[volunteer:",
        stop:  "[/volunteer]",
        rawContents: true,
        emitter: function(contents) {
            var matches = contents.match(/(.+)]([\s\S]*)/);
            var type = matches[1];
            var id = matches[2];
            return ['button', {'id': id,'volunteer-type': type} ].concat('volunteer');
        }
    });
  Discourse.Markdown.whiteListTag('button', 'id');

}).call(this);